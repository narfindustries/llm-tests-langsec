#Zeek Spicy specification for SQLite database file format
type SQLite_DB_meta = record {
    header: SQLite_DB_header,
    page_size: uint16,
    page_count: uint64,
    free_page_list: uint32,
    schema_cookie: uint32,
    schema_format: uint32,
    page_cache_size: uint32,
    largest_btree_page: uint32,
    text_encoding: uint32,
    user_version: uint32,
    incremental_vacuum: uint32,
    application_id: uint32,
    reserved: uint32,
    version_valid_for: uint32,
    sqlite_version: uint32
};

type SQLite_DB_header = record {
    magic: string(16) = "SQLite format 3",
    page_size: uint16,
    write_version: uint8,
    read_version: uint8,
    reserved: uint8[2],
    maxEmbeddedPayloadFraction: uint8,
    minEmbeddedPayloadFraction: uint8,
    leafPayloadFraction: uint8,
    fileChangeCounter: uint32,
    numberNamedColumns: uint32
};