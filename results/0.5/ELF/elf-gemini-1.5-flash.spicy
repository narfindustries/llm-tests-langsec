module ELF;

type elf_ident_entry = enum { EI_MAG0, EI_MAG1, EI_MAG2, EI_MAG3, EI_CLASS, EI_DATA, EI_VERSION, EI_OSABI, EI_ABIVERSION, EI_PAD };

type elf_ident = vector<16> of uint8;

type elf_header = record {
	e_ident: elf_ident;
	e_type: uint16;
	e_machine: uint16;
	e_version: uint32;
	e_entry: uint64;
	e_phoff: uint64;
	e_shoff: uint64;
	e_flags: uint32;
	e_ehsize: uint16;
	e_phentsize: uint16;
	e_phnum: uint16;
	e_shentsize: uint16;
	e_shnum: uint16;
	e_shstrndx: uint16;
};

type elf_program_header = record {
	p_type: uint32;
	p_flags: uint32;
	p_offset: uint64;
	p_vaddr: uint64;
	p_paddr: uint64;
	p_filesz: uint64;
	p_memsz: uint64;
	p_align: uint64;
};

type elf_section_header = record {
	sh_name: uint32;
	sh_type: uint32;
	sh_flags: uint64;
	sh_addr: uint64;
	sh_offset: uint64;
	sh_size: uint64;
	sh_link: uint32;
	sh_info: uint32;
	sh_addralign: uint64;
	sh_entsize: uint64;
};

type elf_file = record {
    header: elf_header;
    program_headers: vector<uint16> of elf_program_header;
    section_headers: vector<uint16> of elf_section_header;
};
