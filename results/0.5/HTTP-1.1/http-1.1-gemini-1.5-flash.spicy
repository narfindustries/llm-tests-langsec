module HTTP_1_1;

type HTTP_Message {
    start_time: time;
    method: string;
    uri: string;
    http_version: string;
    headers: table[string, string];
    body: string;
};

type HTTP_Request extends HTTP_Message {
    cache_control: string;
    connection: string;
    date: time;
    pragma: string;
    trailer: string;
    transfer_encoding: string;
    upgrade: string;
    via: string;
    warning: string;
    accept: string;
    accept_charset: string;
    accept_encoding: string;
    accept_language: string;
    authorization: string;
    expect: string;
    from: string;
    host: string;
    if_match: string;
    if_modified_since: time;
    if_none_match: string;
    if_range: string;
    if_unmodified_since: time;
    max_forwards: int;
    proxy_authorization: string;
    range: string;
    referer: string;
    te: string;
    user_agent: string;
};

type HTTP_Response extends HTTP_Message {
    status_code: int;
    status_text: string;
    cache_control: string;
    connection: string;
    date: time;
    pragma: string;
    trailer: string;
    transfer_encoding: string;
    upgrade: string;
    via: string;
    warning: string;
    accept_ranges: string;
    age: int;
    etag: string;
    location: string;
    proxy_authenticate: string;
    retry_after: time;
    server: string;
    set_cookie: string;
    vary: string;
    www_authenticate: string;
    allow: string;
    content_encoding: string;
    content_language: string;
    content_length: int;
    content_location: string;
    content_md5: string;
    content_range: string;
    content_type: string;
    expires: time;
    last_modified: time;
};
