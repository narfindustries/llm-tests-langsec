          <xs:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:text lengthKind="delimited" lengthUnits="characters" separator=":"/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="value" type="xs:string">
        <xs:annotation>
          <xs:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:text lengthKind="delimited" lengthUnits="characters" termination="%CRLF"/>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:annotation>
      <xs:appinfo source="http://www.ogf.org/dfdl/">
        <dfdl:text lengthKind="delimited" lengthUnits="characters" termination="%CRLF"/>
      </xs:appinfo>
    </xs:annotation>
  </xs:complexType>
