<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/" targetNamespace="http://example.org/http" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:annotation>
    <xs:appinfo source="http://www.ogf.org/dfdl/">
      <dfdl:defineSchema version="1.0"/>
    </xs:appinfo>
  </xs:annotation>
  <xs:element name="http-1">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="header" type="headerType" maxOccurs="unbounded"/>
        <xs:element name="body" type="xs:string"/>
      </xs:sequence>
      <xs:attribute name="version" type="xs:string" use="required">
        <xs:annotation>
          <xs:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:text lengthKind="delimited" lengthUnits="bytes"/>
          </xs:appinfo>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
    <xs:annotation>
      <xs:appinfo source="http://www.ogf.org/dfdl/">
