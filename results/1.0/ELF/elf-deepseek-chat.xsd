<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
           xmlns:ex="http://example.com/elf-deepseek-chat"
           targetNamespace="http://example.com/elf-deepseek-chat"
           elementFormDefault="qualified">

  <xs:annotation>
    <xs:appinfo source="http://www.ogf.org/dfdl/">
      <dfdl:format ref="ex:ELFDeepSeekChatFormat"/>
    </xs:appinfo>
  </xs:annotation>

  <xs:element name="ELFDeepSeekChat" type="ex:ELFDeepSeekChatType"/>

  <xs:complexType name="ELFDeepSeekChatType">
    <xs:sequence>
      <xs:element name="Header" type="ex:HeaderType"/>
      <xs:element name="Sections" type="ex:SectionsType"/>
      <xs:element name="Symbols" type="ex:SymbolsType"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="HeaderType">
    <xs:sequence>
      <xs:element name="MagicNumber" type="xs:hexBinary" dfdl:length="4"/>
      <xs:element name="Class" type="xs:unsignedByte"/>
      <xs:element name="DataEncoding" type="xs:unsignedByte"/>
      <xs:element name="Version" type="xs:unsignedByte"/>
      <xs:element name="OSABI" type="xs:unsignedByte"/>
      <xs:element name="ABIVersion" type="xs:unsignedByte"/>
      <xs:element name="Padding" type="xs:hexBinary" dfdl:length="7"/>
      <xs:element name="Type" type="xs:unsignedShort"/>
      <xs:element name="Machine" type="xs:unsignedShort"/>
      <xs:element name="Version2" type="xs:unsignedInt"/>
      <xs:element name="EntryPoint" type="xs:unsignedLong"/>
      <xs:element name="ProgramHeaderOffset" type="xs:unsignedLong"/>
      <xs:element name="SectionHeaderOffset" type="xs:unsignedLong"/>
      <xs:element name="Flags" type="xs:unsignedInt"/>
      <xs:element name="HeaderSize" type="xs:unsignedShort"/>
      <xs:element name="ProgramHeaderEntrySize" type="xs:unsignedShort"/>
      <xs:element name="ProgramHeaderCount" type="xs:unsignedShort"/>
      <xs:element name="SectionHeaderEntrySize" type="xs:unsignedShort"/>
      <xs:element name="SectionHeaderCount" type="xs:unsignedShort"/>
      <xs:element name="SectionHeaderStringIndex" type="xs:unsignedShort"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SectionsType">
    <xs:sequence>
      <xs:element name="Section" type="ex:SectionType" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SectionType">
    <xs:sequence>
      <xs:element name="NameIndex" type="xs:unsignedInt"/>
      <xs:element name="Type" type="xs:unsignedInt"/>
      <xs:element name="Flags" type="xs:unsignedLong"/>
      <xs:element name="Address" type="xs:unsignedLong"/>
      <xs:element name="Offset" type="xs:unsignedLong"/>
      <xs:element name="Size" type="xs:unsignedLong"/>
      <xs:element name="Link" type="xs:unsignedInt"/>
      <xs:element name="Info" type="xs:unsignedInt"/>
      <xs:element name="AddressAlign" type="xs:unsignedLong"/>
      <xs:element name="EntrySize" type="xs:unsignedLong"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SymbolsType">
    <xs:sequence>
      <xs:element name="Symbol" type="ex:SymbolType" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="SymbolType">
    <xs:sequence>
      <xs:element name="NameIndex" type="xs:unsignedInt"/>
      <xs:element name="Value" type="xs:unsignedLong"/>
      <xs:element name="Size" type="xs:unsignedLong"/>
      <xs:element name="Info" type="xs:unsignedByte"/>
      <xs:element name="Other" type="xs:unsignedByte"/>
      <xs:element name="SectionIndex" type="xs:unsignedShort"/>
    </xs:sequence>
  </xs:complexType>

  <dfdl:defineFormat name="ELFDeepSeekChatFormat">
    <dfdl:format representation="binary" byteOrder="littleEndian"/>
  </dfdl:defineFormat>

</xs:schema>