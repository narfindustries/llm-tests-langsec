module HL7v2;

public type MSH = unit {
    field_separator:      bytes &size=1;
    encoding_characters: bytes &size=4;
    sending_application:  bytes &until=b"|";
    sending_facility:     bytes &until=b"|";
    receiving_application: bytes &until=b"|";
    receiving_facility:    bytes &until=b"|";
    datetime:            bytes &until=b"|";
    security:            bytes &until=b"|";
    message_type:        bytes &until=b"|";
    message_control_id:   bytes &until=b"|";
    processing_id:        bytes &until=b"|";
    version_id:          bytes &until=b"|";
    sequence_number:     bytes &until=b"|";
    continuation_pointer: bytes &until=b"|";
    accept_ack_type:      bytes &until=b"|";
    application_ack_type: bytes &until=b"|";
    country_code:        bytes &until=b"|";
    character_set:       bytes &until=b"|";
    principal_language:   bytes &until=b"|";
    alternate_character_set: bytes &until=b"|";
    message_profile_id: bytes &until=b"|";
};

public type PID = unit {
    set_id:              bytes &until=b"|";
    patient_id:          bytes &until=b"|";
    patient_identifier_list: bytes &until=b"|";
    alternate_patient_id: bytes &until=b"|";
    patient_name:        bytes &until=b"|";
    mother_maiden_name:  bytes &until=b"|";
    datetime_of_birth:   bytes &until=b"|";
    sex:                bytes &until=b"|";
    patient_alias:      bytes &until=b"|";
    race:               bytes &until=b"|";
    patient_address:    bytes &until=b"|";
    county_code:        bytes &until=b"|";
    phone_number_home:  bytes &until=b"|";
    phone_number_business: bytes &until=b"|";
    primary_language:   bytes &until=b"|";
    marital_status:     bytes &until=b"|";
    religion:           bytes &until=b"|";
    patient_account_number: bytes &until=b"|";
    ssn_number:         bytes &until=b"|";
    driver_license_number: bytes &until=b"|";
    mother_identifier:  bytes &until=b"|";
    ethnic_group:       bytes &until=b"|";
    birth_place:       bytes &until=b"|";
    multiple_birth_indicator: bytes &until=b"|";
    birth_order:       bytes &until=b"|";
    citizenship:       bytes &until=b"|";
    veterans_military_status: bytes &until=b"|";
    nationality:       bytes &until=b"|";
    patient_death_datetime: bytes &until=b"|";
    patient_death_indicator: bytes &until=b"|";
    identity_unknown_indicator: bytes &until=b"|";
    identity_reliability_code: bytes &until=b"|";
    last_update_datetime: bytes &until=b"|";
    last_update_facility: bytes &until=b"|";
    species_code:      bytes &until=b"|";
    breed_code:        bytes &until=b"|";
    strain:            bytes &until=b"|";
    production_class_code: bytes &until=b"|";
    tribal_citizenship: bytes &until=b"|";
    patient_telecommunication_information: bytes &until=b"|";
};

public type PV1 = unit {
    set_id:              bytes &until=b"|";
    patient_class:       bytes &until=b"|";
    assigned_patient_location: bytes &until=b"|";
    admission_type:      bytes &until=b"|";
    preadmit_number:     bytes &until=b"|";
    prior_patient_location: bytes &until=b"|";
    attending_doctor:   bytes &until=b"|";
    referring_doctor:    bytes &until=b"|";
    consulting_doctor:  bytes &until=b"|";
