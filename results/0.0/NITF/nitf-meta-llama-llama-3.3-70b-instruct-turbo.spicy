# NITF (National Image Transmission Format) meta file parser

# Define the NITF file header
file NITF_META {
    # File header
    header: NITF_Header;

    # File body
    body: NITF_Body;
}

# Define the NITF file header structure
type NITF_Header {
    # NITF file header fields
    file_id: bytestring(25);
    file_date: bytestring(10);
    file_time: bytestring(8);
    file_title: bytestring(80);
    file_security: bytestring(80);
    file_info: bytestring(255);
}

# Define the NITF file body structure
type NITF_Body {
    # NITF file body fields
    image_data: bytestring;
}

# Define the NITF image data structure
type NITF_Image_Data {
    # NITF image data fields
    image_id: bytestring(25);
    image_date: bytestring(10);
    image_time: bytestring(8);
    image_data: bytestring;
}

# Define the NITF meta data structure
type NITF_Meta {
    # NITF meta data fields
    meta_id: bytestring(25);
    meta_date: bytestring(10);
    meta_time: bytestring(8);
    meta_data: bytestring;
}