<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
           xmlns:daf="http://www.ogf.org/dfdl/dfdl-1.0/"
           targetNamespace="http://www.example.com/gzip-deepseek-chat"
           xmlns="http://www.example.com/gzip-deepseek-chat"
           elementFormDefault="qualified">

  <xs:annotation>
    <xs:appinfo source="http://www.ogf.org/dfdl/">
      <dfdl:format ref="tns:GeneralFormat"/>
    </xs:appinfo>
  </xs:annotation>

  <xs:complexType name="GZIPType">
    <xs:sequence>
      <xs:element name="ID1" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="ID2" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="CompressionMethod" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="Flags" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="ModificationTime" type="xs:unsignedInt" dfdl:length="4" dfdl:lengthKind="explicit"/>
      <xs:element name="ExtraFlags" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="OperatingSystem" type="xs:unsignedByte" dfdl:length="1" dfdl:lengthKind="explicit"/>
      <xs:element name="ExtraField" dfdl:lengthKind="implicit">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ExtraFieldLength" type="xs:unsignedShort" dfdl:length="2" dfdl:lengthKind="explicit"/>
            <xs:element name="ExtraFieldData" type="xs:hexBinary" dfdl:length="{../ExtraFieldLength}" dfdl:lengthKind="explicit"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="OriginalFileName" dfdl:lengthKind="delimited" dfdl:terminator="%#x00;"/>
      <xs:element name="Comment" dfdl:lengthKind="delimited" dfdl:terminator="%#x00;"/>
      <xs:element name="CRC16" type="xs:unsignedShort" dfdl:length="2" dfdl:lengthKind="explicit"/>
      <xs:element name="CompressedData" type="xs:hexBinary" dfdl:lengthKind="implicit"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="GZIP" type="GZIPType"/>

  <dfdl:defineFormat name="GeneralFormat">
    <dfdl:format representation="binary" byteOrder="littleEndian" encoding="ISO-8859-1"/>
  </dfdl:defineFormat>

</xs:schema>