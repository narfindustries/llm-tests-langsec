<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
           xmlns:daf="http://www.ogf.org/dfdl/dfdl-1.0/"
           targetNamespace="http://www.ogf.org/dfdl/dfdl-1.0/"
           elementFormDefault="qualified">

  <xs:include schemaLocation="dfdl-general-format.dfdl.xsd"/>

  <xs:element name="ELF" dfdl:lengthKind="implicit">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="e_ident" dfdl:length="16">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="magic" dfdl:length="4" dfdl:encoding="ASCII" dfdl:initiator="0x7F"/>
              <xs:element name="class" dfdl:length="1" dfdl:encoding="ASCII"/>
              <xs:element name="data" dfdl:length="1" dfdl:encoding="ASCII"/>
              <xs:element name="version" dfdl:length="1" dfdl:encoding="ASCII"/>
              <xs:element name="osabi" dfdl:length="1" dfdl:encoding="ASCII"/>
              <xs:element name="abiversion" dfdl:length="1" dfdl:encoding="ASCII"/>
              <xs:element name="pad" dfdl:length="7" dfdl:encoding="ASCII"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="e_type" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_machine" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_version" dfdl:length="4" dfdl:encoding="ASCII"/>
        <xs:element name="e_entry" dfdl:length="4" dfdl:encoding="ASCII"/>
        <xs:element name="e_phoff" dfdl:length="4" dfdl:encoding="ASCII"/>
        <xs:element name="e_shoff" dfdl:length="4" dfdl:encoding="ASCII"/>
        <xs:element name="e_flags" dfdl:length="4" dfdl:encoding="ASCII"/>
        <xs:element name="e_ehsize" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_phentsize" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_phnum" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_shentsize" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_shnum" dfdl:length="2" dfdl:encoding="ASCII"/>
        <xs:element name="e_shstrndx" dfdl:length="2" dfdl:encoding="ASCII"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>