meta:
  id: hl7v2
  title: HL7 Version 2.x
  license: CC0-1.0
  endian: be
  encoding: ASCII
seq:
  - id: segments
    type: segment
    repeat: eos
types:
  segment:
    seq:
      - id: segment_id
        type: str
        size: 3
      - id: fields
        type: field
        repeat: until
        repeat-until: _io.eof
  field:
    seq:
      - id: components
        type: component
        repeat: until
        repeat-until: _io.eof
  component:
    seq:
      - id: subcomponents
        type: subcomponent
        repeat: until
        repeat-until: _io.eof
  subcomponent:
    seq:
      - id: value
        type: str
        size-eos: true
